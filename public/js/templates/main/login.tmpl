<div class="tr">
	<span class="text-muted">PLEASE LOG IN BELOW</span>
	<form id="login-form" action="/login" method="post" class="form">
		<div class="form-group">
			<input type="text" class="form-control" id="fqdn" name="fqdn" placeholder="Fully qualified domain name of Gitlab server">
		</div>
		<div class="form-group">
			<input type="text" class="form-control" id="username" name="username" placeholder="GitLab Username">
		</div>
		<div class="form-group">
			<input type="text" class="form-control" id="private-token" name="private-token" placeholder="Private Token">
			<span class="help-block">Your private token can be found under the "My Profile" section under the "Account" tab.</span>
		</div>
		<div class="form-group" style="text-align: left;">
			<button type="submit" id="login-btn" class="btn btn-primary pull-right">Sign In</button>
		</div>
	</form>
</div>
